{
  "coc.preferences.jumpCommand": "tab drop",
  "languageserver": {
    "vlang": {
      "command": "vls",
      "filetypes": [
        "vlang"
      ]
    },
    "erlang": {
      "command": "erlang_ls",
      "filetypes": [
        "erlang"
      ]
    },
    "zls": {
      "command": "zls",
      "filetypes": [
        "zig"
      ]
    }
  },
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
